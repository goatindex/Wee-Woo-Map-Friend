# Environment Configuration
NODE_ENV=development

# API Keys
VITE_WILLYWEATHER_API_KEY=your_willyweather_api_key_here
VITE_EMV_API_KEY=your_emv_api_key_here
VITE_MAPBOX_TOKEN=your_mapbox_token_here

# API Endpoints
VITE_WEATHER_API_URL=https://api.willyweather.com.au/v1
VITE_EMV_API_URL=https://api.emergency.vic.gov.au
VITE_ROUTING_API_URL=https://api.mapbox.com/directions/v5

# Application Settings
VITE_APP_TITLE=WeeWoo Map Friends V2
VITE_APP_VERSION=2.0.0
VITE_DEFAULT_MAP_CENTER_LAT=-37.8136
VITE_DEFAULT_MAP_CENTER_LON=144.9631
VITE_DEFAULT_MAP_ZOOM=8

# Feature Flags
VITE_ENABLE_WEATHER=true
VITE_ENABLE_ALERTS=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_OFFLINE=true

# Performance Settings
VITE_CACHE_DURATION=3600000
VITE_MAX_CACHE_SIZE=50
VITE_DEBOUNCE_DELAY=300

