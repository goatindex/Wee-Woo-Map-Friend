{
  "name": "weewoo-map-friend",
  "private": true,
  "type": "module",
  "version": "2.0.0",
  "description": "Interactive emergency services mapping for Victoria, Australia - now with native app support",
  "license": "MIT",
  "scripts": {
    "capture": "node scripts/capture.js",
    "dev": "npm run build:js && concurrently \"npm run watch:js\" \"npm run serve:dev\"",
    "dev:simple": "python -m http.server 8000",
    "build": "npm run build:js && npm run build:web && npm run build:native",
    "build:js": "swc js/modules --out-dir dist --strip-leading-paths --source-maps",
    "build:web": "echo 'Web build: Static files ready for GitHub Pages'",
    "build:native": "npm run cap:copy && npm run cap:build",
    "watch:js": "swc js/modules --out-dir dist --strip-leading-paths --watch --source-maps",
    "serve:dev": "python -m http.server 8000",
    "serve:dist": "cd dist && python -m http.server 8001",
    "cap:init": "cap init",
    "cap:add": "cap add ios && cap add android",
    "cap:copy": "cap copy",
    "cap:sync": "cap sync",
    "cap:build": "cap build",
    "cap:run:ios": "cap run ios",
    "cap:run:android": "cap run android",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "icons": "node scripts/generate-icons.js",
    "test": "npm run test:e2e && npm run test:unit",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "test:watch": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "analyze:legacy": "node scripts/analyze-legacy-usage.js",
    "test:core": "playwright test tests/core/",
    "test:performance": "playwright test tests/performance/",
    "test:compatibility": "playwright test tests/compatibility/",
    "test:debug": "playwright test tests/debug/",
    "test:pwa": "npx serve . -p 8000",
    "test:report": "playwright show-report",
    "lint": "echo 'Linting not configured yet'",
    "quality:check": "npm run lint && npm run test",
    "quality:report": "npm run test && npm run lint",
    "prepare:appstore": "npm run build && npm run icons && npm run cap:sync",
    "prebuild": "npm run clean:dist",
    "clean:dist": "rimraf dist",
    "precommit": "npm run build:js && npm run test"
  },
  "devDependencies": {
    "@capacitor/android": "^5.4.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/cli": "^5.4.0",
    "@capacitor/core": "^5.4.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.4.0",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@playwright/test": "^1.55.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "concurrently": "^8.2.2",
    "lighthouse": "^12.8.2",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.0",
    "puppeteer": "^24.18.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.32.0"
  },
  "keywords": [
    "emergency-services",
    "mapping",
    "victoria",
    "australia",
    "pwa",
    "mobile-app",
    "leaflet",
    "geojson"
  ],
  "homepage": "https://goatindex.github.io/mapexp.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/goatindex/mapexp.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/goatindex/mapexp.github.io/issues"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "inversify": "^7.9.1",
    "react-redux": "^9.2.0",
    "reflect-metadata": "^0.2.2"
  }
}
