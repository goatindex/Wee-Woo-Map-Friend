{
  "name": "weewoo-map-friend",
  "private": true,
  "version": "2.0.0",
  "description": "Interactive emergency services mapping for Victoria, Australia - now with native app support",
  "license": "MIT",
  "scripts": {
    "capture": "node scripts/capture.js",
    "dev": "python -m http.server 8000",
    "build": "npm run build:web && npm run build:native",
    "build:web": "echo 'Web build: Static files ready for GitHub Pages'",
    "build:native": "npm run cap:copy && npm run cap:build",
    "cap:init": "cap init",
    "cap:add": "cap add ios && cap add android",
    "cap:copy": "cap copy",
    "cap:sync": "cap sync",
    "cap:build": "cap build",
    "cap:run:ios": "cap run ios",
    "cap:run:android": "cap run android",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "icons": "node scripts/generate-icons.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:es6": "jest --testPathPattern=modules",
    "test:es6:watch": "jest --testPathPattern=modules --watch",
    "test:es6:coverage": "jest --testPathPattern=modules --coverage",
    "test:legacy": "jest --testPathIgnorePatterns=modules",
    "test:all": "jest --passWithNoTests",
    "analyze:legacy": "node scripts/analyze-legacy-usage.js",
    "test:phase1": "jest tests/Phase1TestFramework.test.js",
    "test:performance": "jest tests/performance/",
    "test:performance:suite": "jest tests/performance/PerformanceTestSuite.test.js",
    "test:performance:budgets": "jest --testNamePattern='Performance Budget'",
    "test:runtime-health": "node scripts/check-performance-budget.js",
    "test:pwa": "npx serve . -p 8000",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:progress": "node tests/e2e/run-with-progress.js",
    "test:e2e:progress:verbose": "node tests/e2e/run-with-progress.js --verbose",
    "test:e2e:progress:headed": "node tests/e2e/run-with-progress.js --headed",
    "test:e2e:progress:firefox": "node tests/e2e/run-with-progress.js --browser firefox",
    "test:e2e:progress:webkit": "node tests/e2e/run-with-progress.js --browser webkit",
    "lint": "echo 'Linting not configured yet'",
    "quality:check": "npm run lint && npm run test:coverage",
    "quality:report": "npm run test:coverage && npm run lint",
    "budget:check": "node scripts/check-performance-budget.js",
    "prepare:appstore": "npm run build && npm run icons && npm run cap:sync"
  },

  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@capacitor/android": "^5.4.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/cli": "^5.4.0",
    "@capacitor/core": "^5.4.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/geolocation": "^5.0.6",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.4.0",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@playwright/test": "^1.55.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.2",
    "playwright": "^1.55.0",
    "puppeteer": "^24.18.0",
    "sharp": "^0.32.0"
  },
  "keywords": [
    "emergency-services",
    "mapping",
    "victoria",
    "australia",
    "pwa",
    "mobile-app",
    "leaflet",
    "geojson"
  ],
  "homepage": "https://goatindex.github.io/mapexp.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/goatindex/mapexp.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/goatindex/mapexp.github.io/issues"
  }
}
